<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.demo.controllers.TelaInicialController">
   <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

    <Label text="Academia" style="-fx-font-size: 20px;"/>

    <Button text="Abrir Alunos" onAction="#onAbrirAlunosButtonClick"/>
    <Button text="Novo Aluno" onAction="#onNovoAlunoButtonClick"/>
    <Button text="Editar Aluno" onAction="#onEditarAlunoButtonClick"/>
    <Button text="Excluir Aluno" onAction="#onExcluirAlunoButtonClick"/>

</VBox>
